{
    "name": "delivermyride",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "husky": "^0.14.3",
        "lint-staged": "^7.2.2",
        "prettier": "1.14.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,css,scss}": [
            "prettier --write",
            "git add"
        ]
    }
}
